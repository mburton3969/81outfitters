<footer>
	<div class="row">	
	{% if footer_1 %} 
		<div class="col-sm-12 footer_modules">{{footer_1}}</div>
	{% endif %}
	</div>
	<div class="container">		
		<div class="row">	
			{% if footer_2 %}
				<div class="col-xs-12 col-sm-3 col-md-3 col-lg-3 footer_modules">{{footer_2}}</div>
			{% endif %}
			{% if footer_3 %}
				<div class="col-xs-12 col-sm-3 col-md-3 col-lg-3 footer_modules">{{footer_3}}</div>
			{% endif %}
			{% if footer_4 %}
				<div class="col-xs-12 col-sm-3 col-md-3 col-lg-3 footer_modules">{{footer_4}}</div>
			{% endif %}
			{% if footer_5 %}
				<div class="col-xs-12 col-sm-3 col-md-3 col-lg-3 footer_modules">{{footer_5}}</div>
			{% endif %}	
		</div>
	</div>
</footer>
<div class="ajax-overlay"></div>
<div class="ajax-quickview-overlay">
	<span class="ajax-quickview-overlay__preloader"></span>
</div>
{% for script in scripts %}
	<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
<script src="catalog/view/theme/{{ theme_path }}/js/device.min.js" type="text/javascript"></script>
<script src="catalog/view/theme/{{ theme_path }}/js/livesearch.min.js" type="text/javascript"></script>
<script src="catalog/view/theme/{{ theme_path }}/js/common.js" type="text/javascript"></script>
<script src="catalog/view/theme/{{ theme_path }}/js/script.js" type="text/javascript"></script>
</div>
</body>
</html>